!!!
%html
  %head
    %link{:href => "/assets/favicon.ico",:rel => "shortcut icon"}
    %title Cloud Foundry Developer Group
    = stylesheet_link_tag    "application", :media => "all"
    = stylesheet_link_tag    "create_chapter_style", :media => "all"   

    = javascript_include_tag "application"    
    %script{:src => "http://commons.qiniudn.com/jquery-1.8.3.min.js"}
    %script{:src => "http://commons.qiniudn.com/jquery-ui-1.9.2.custom.min.js"}

    = csrf_meta_tags
  %body
    = render "shared/header"
    = yield
    = render "shared/footer"
    %script{:src => "http://commons.qiniudn.com/jquery-1.8.3.min.js"}
    %script{:src => "http://commons.qiniudn.com/jquery-ui-1.9.2.custom.min.js"}
    = javascript_include_tag :application
    = yield :scripts
